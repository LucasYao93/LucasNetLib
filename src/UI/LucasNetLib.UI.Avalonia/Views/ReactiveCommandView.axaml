<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:vm="using:LucasNetLib.UI.Avalonia.ViewModels"
             x:DataType="vm:ReactiveCommandViewModel"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="LucasNetLib.UI.Avalonia.Views.ReactiveCommandView">
  <StackPanel Margin="20">
    <TextBlock Margin="0 5" >User Name</TextBlock>
    <TextBox Text="{Binding UserName}"/>
    <Button Margin="0 20" Command="{Binding SubmitCommand}">Submit</Button>
  </StackPanel>
</UserControl>
