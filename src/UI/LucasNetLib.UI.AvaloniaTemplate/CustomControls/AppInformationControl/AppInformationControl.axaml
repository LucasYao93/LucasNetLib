<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="LucasNetLib.UI.AvaloniaTemplate.CustomControls.AppInformationControl">
  
  <Grid RowDefinitions="1" ColumnDefinitions="2" >
    <Image Grid.Row="1" Grid.Column="1"></Image>
    <Grid ColumnDefinitions="1" RowDefinitions="2">
      <TextBox Grid.Row="1" Grid.Column="1"></TextBox>
      <TextBox Grid.Row="1" Grid.Column="2"></TextBox>
    </Grid>
  </Grid>
  
</UserControl>
